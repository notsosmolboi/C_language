        for(int i=0;i<l;i++)
            If(isupper(a[i]))
                up++;
            else if(islower(a[i]))
                lp++;
            else if(isdigit(a[i]))
                dg++;
            else
                sp++;
         if(up>0&&lp>0&&dg>0&&sp>0)
             printf("STRONG PASSWORD");
