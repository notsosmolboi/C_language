#include<stdio.h>
#include<conio.h>
void main(){
    int a,b,c;
    printf("Enter a,b,c : ");
    scanf("%d%d%d",&a&b&c);
    if(a>b && a>c){
        printf("%d is the largest",a);
        if(b>c)
             printf("%d is second largest",b);
        else
             printf("%d is second largest",c);
    }
    else if(b>c && b>a){
        printf("%d is the largest",b);
        if(a>c)
             printf("%d is second largest",a);
        else
             printf("%d is second largest",c);
    }
    else if(c>a && c>b){
        printf("%d is the largest",c);
        if(a>b)
             printf("%d is second largest",a);
        else
             printf("%d is second largest",b);
    }
}