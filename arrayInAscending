#include<stdio.h>
void main(){
    int a[10];
    int n,temp;
    printf("Enter number of elements : ");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        printf("\nEnter %d number in array : ",i);
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    for(int i=0;i<n;i++){
        printf(" %d ",a[i]);
}
}
